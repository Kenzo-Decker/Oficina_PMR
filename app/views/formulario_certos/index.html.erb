<h1>Formulario Certos</h1>

<table>
  <thead>
    <tr>
      <th>Data</th>
      <th>Departamento</th>
      <th>Nome projeto</th>
      <th>Tipo projeto string</th>
      <th>Orientador</th>
      <th>Maquina</th>
      <th>Ferramentas proprias</th>
      <th>Estado</th>
      <th>Material</th>
      <th>Desenho peca</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @formulario_certos.each do |formulario_certo| %>
      <tr>
        <td><%= formulario_certo.data %></td>
        <td><%= formulario_certo.departamento %></td>
        <td><%= formulario_certo.nome_projeto %></td>
        <td><%= formulario_certo.tipo_projeto_string %></td>
        <td><%= formulario_certo.orientador %></td>
        <td><%= formulario_certo.maquina %></td>
        <td><%= formulario_certo.ferramentas_proprias %></td>
        <td><%= formulario_certo.estado %></td>
        <td><%= formulario_certo.material %></td>
        <td><%= formulario_certo.desenho_peca %></td>
        <td><%= link_to 'Show', formulario_certo %></td>
        <td><%= link_to 'Edit', edit_formulario_certo_path(formulario_certo) %></td>
        <td><%= link_to 'Destroy', formulario_certo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Formulario Certo', new_formulario_certo_path %>
